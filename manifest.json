{
   "manifest_version": 3,
   "name": "Keyword Bookmarks",
   "version": "0.0.0.1",
   "description": "Quickly open a bookmark from the keyboard based on a keyword in the bookmark title.",
   "background": {
      "service_worker": "./scripts/background.js",
      "type": "module"
   },
   "icons": {
      "16": "images/kwbm16.png",
      "48": "images/kwbm48.png",
      "128": "images/kwbm128.png"
   },
   "omnibox": {
      "keyword": "kw"
   },
   "permissions": [
     "bookmarks"
     , "storage"
     , "tabs"
   ]
}
